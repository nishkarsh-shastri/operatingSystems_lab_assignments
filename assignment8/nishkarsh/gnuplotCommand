set terminal png font 'Helvetica,20' size 1024,768 linewidth 3
set output 'all_in_one.png'
set key font ",20"
set xlabel 'Page Table Size(m)'
set ylabel 'Page Faults'
set autoscale
set title 'Page Faults Comparison Plot'
plot "sca.dat" every ::2 u 1:2 with lines,"lru.dat" every ::2 u 1:2 with lines,"fifo.dat" every ::2 u 1:2 with lines,"lfu.dat" every ::2 u 1:2 with lines
