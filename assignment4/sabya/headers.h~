#include <time.h>
#define UP 			8
#define DOWN 		9
#define TYPE_SIZE 	10
#define DATA_SIZE 	256
#define ID_SIZE 	20
#define TIME_SIZE	20
#define MSG_SIZE 	300
#define MAX_CLIENTS 25

typedef struct mymsgbuf {
	long mtype;
	char dtype[TYPE_SIZE];
	char data[DATA_SIZE];
	char chatID[ID_SIZE];
	char sd_time_buf[TIME_SIZE];
}mymsgbuf;

